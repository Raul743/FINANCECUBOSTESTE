{
  "name": "finance-cubos-api",
  "version": "1.0.0",
  "description": "API for FINANCE CUBOS APP",
  "main": "build/server.js",
  "repository": "git@github.com:Raul743/FINANCECUBOSTESTE.git",
  "author": "Raul Inacio <raulinacio233@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "node .",
    "dev": "ts-node-dev --inspect -r tsconfig-paths/register --respawn --transpile-only --ignore-watch node_modules --no-notify src/server.ts",
    "lint": "eslint --fix --ext .ts,.tsx ./src",
    "build": "rimraf build && babel src --extensions \".js,.ts\" --out-dir build --copy-files --no-copy-ignored",
    "typeorm": "ts-node ./node_modules/typeorm/cli",
    "typeorm:run-migrations": "npm run typeorm migration:run -- -d ./src/configs/db/postgres/index.ts",
    "typeorm:generate-migration": "yarn typeorm -- -d ./src/configs/db/postgres/index.ts migration:generate./src/configs/db/postgres/migrations/$npm_config_name",
    "typeorm:create-migration": "yarn typeorm -- migration:create ./src/configs/db/postgres/migrations/$npm_config_name",
    "typeorm:revert-migration": "yarn run typeorm -- -d ./src/configs/db/postgres/index.ts migration:revert",
    "test": "jest"
  },
  "dependencies": {
    "@sendgrid/mail": "^7.7.0",
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.12",
    "@types/dotenv-safe": "^8.1.2",
    "@types/express": "^4.17.13",
    "@types/morgan": "^1.9.3",
    "@types/multer": "^1.4.7",
    "@types/nodemailer": "^6.4.4",
    "@types/shelljs": "^0.8.11",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "@typescript-eslint/parser": "^5.26.0",
    "axios": "^0.27.2",
    "basic-ftp": "^5.0.1",
    "bcrypt": "^5.0.1",
    "canvas": "^2.9.3",
    "cors": "^2.8.5",
    "dayjs": "^1.11.4",
    "dotenv-safe": "^8.2.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-import-helpers": "^1.2.1",
    "eslint-plugin-prettier": "^4.0.0",
    "express": "^4.18.1",
    "express-async-errors": "^3.1.1",
    "generate-password": "^1.7.0",
    "google-auth-library": "^8.1.1",
    "handlebars": "^4.7.7",
    "html-entities": "^2.3.3",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nodejs": "^0.0.0",
    "nodemailer": "^6.7.5",
    "prettier": "^2.6.2",
    "qrcode": "^1.5.1",
    "reflect-metadata": "^0.1.13",
    "shelljs": "^0.8.5",
    "supertest": "^6.3.0",
    "swagger-ui-express": "^4.4.0",
    "ts-node-dev": "^2.0.0",
    "tsyringe": "^4.7.0",
    "twilio": "^3.78.0",
    "typeorm": "^0.3.6",
    "typeorm-model-generator": "^0.4.6",
    "uuid": "^8.3.2",
    "yup": "^0.32.11",
    "ts-jest": "^29.0.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.10",
    "@babel/core": "^7.18.5",
    "@babel/node": "^7.18.5",
    "@babel/plugin-proposal-class-properties": "^7.17.12",
    "@babel/plugin-proposal-decorators": "^7.18.2",
    "@babel/plugin-transform-flow-strip-types": "^7.17.12",
    "@babel/preset-env": "^7.18.2",
    "@babel/preset-typescript": "^7.17.12",
    "@types/jest": "^29.1.2",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/qrcode": "^1.4.2",
    "@types/supertest": "^2.0.12",
    "@types/typescript": "^2.0.0",
    "@types/uuid": "^8.3.4",
    "@types/yup": "^0.29.14",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-transform-typescript-metadata": "^0.3.2",
    "eslint": "^8.7.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "jest": "^29.1.2",
    "typescript": "^4.7.2"
  }
}
